<p id="notice"><%= notice %></p>

<h1>Poblations</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poblations.each do |poblation| %>
      <tr>
        <td><%= poblation.nombre %></td>
        <td><%= link_to 'Show', poblation %></td>
        <td><%= link_to 'Edit', edit_poblation_path(poblation) %></td>
        <td><%= link_to 'Destroy', poblation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poblation', new_poblation_path %>
