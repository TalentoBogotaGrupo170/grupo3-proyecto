<p id="notice"><%= notice %></p>

<h1>Poblacions</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @poblacions.each do |poblacion| %>
      <tr>
        <td><%= poblacion.nombre %></td>
        <td><%= link_to 'Show', poblacion %></td>
        <td><%= link_to 'Edit', edit_poblacion_path(poblacion) %></td>
        <td><%= link_to 'Destroy', poblacion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Poblacion', new_poblacion_path %>
